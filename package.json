{
  "name": "react-search-suggest",
  "version": "1.5.0",
  "description": "React Search Suggest Example",
  "main": "demo/src/index.js",
  "author": "Amit Bauriya <amit001bauriya@gmail.com>",
  "scripts": {
    "start": "mkdirp demo/public && npm run static-files && node server",
    "build": "npm run dist",
    "prettier": "prettier --single-quote --write \".*.js\" \"*.js\" \"demo/src/**/*.js\" \"src/**/*.js\"",
    "static-files": "cpx demo/src/index.html demo/public/ && cpx demo/css/autosuggest.css demo/public/",
    "dist": "mkdirp dist && cpx src -d dist",
    "demo-dist": "npm run static-files && cross-env BABEL_ENV=production",
    "lint": "eslint src demo/src server.js webpack.*.js",
    "prebuild": "npm run prettier && npm run lint",
    "prepublish": "npm run dist",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "shallow-equal": "^1.0.0"
  },
  "peerDependencies": {
    "react": ">=0.14.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "autosuggest-highlight": "^3.1.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "cpx": "^1.5.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.28.4",
    "es6-promise": "^4.1.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.11.2",
    "husky": "^0.13.4",
    "ismobilejs": "^0.4.1",
    "jsdom": "^9.12.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "lint-staged": "^3.6.1",
    "mkdirp": "^0.5.1",
    "openurl": "^1.1.1",
    "postcss-loader": "^1.3.3",
    "prettier": "1.7.4",
    "react": "^15.6.1",
    "react-autowhatever": "^10.1.0",
    "react-dom": "^15.6.1",
    "react-modal": "^1.7.7",
    "react-transform-hmr": "^1.0.4",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.11.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    ".*.js": [
      "npm run prettier"
    ],
    "*.js": [
      "npm run prettier"
    ],
    "demo/src/**/*.js": [
      "npm run prettier"
    ],
    "src/**/*.js": [
      "npm run prettier"
    ]
  },
  "license": "MIT"
}
